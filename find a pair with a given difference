public class second {
    public static void  diff(int a[])
    {
        for(int i=0,j=i+1;i<a.length-1 && j<a.length-1;i++,j++)
        {
            if(a[i]-a[j]==-2)
            {
                System.out.println("("+a[i]+","+a[j]+")");
            }
        }
    }
    public static void main(String[] args) {
        int a[] ={1, 1, 2, 1, 3, 5, 1,2};
        diff(a);
    }
}
